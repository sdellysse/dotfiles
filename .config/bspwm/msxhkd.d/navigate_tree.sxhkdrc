# Navigate to the parent node
p
  bspc node -f @parent

# Navigate to the first child
bracketleft
  bspc node -f @first

# Navigatie to the last child
bracketright
  bspc node -f @second

# Navigate to the other sibling
o
  bspc node -f @brother

Escape
  ~/.config/bspwm/bspwmrc.d/set_border regular && msxhkd set-mode default
