#!/bin/bash

if [ $(os) = 'Windows' ]; then
    export DISPLAY=:0.0

    if [ -x /usr/local/bin/ssh-pageant ]; then
        eval $(/usr/local/bin/ssh-pageant -qra /tmp/.ssh-pageant)
    else
        echo "Install ssh-pagent"
    fi

    #if [ -z "$SSH_AUTH_SOCK" -a -x /usr/local/bin/ssh-pageant ]; then
    #    eval $(/usr/local/bin/ssh-pageant -q)
    #fi
fi
